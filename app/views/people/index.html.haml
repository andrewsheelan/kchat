.page-header
  %h1 Listing people

.table-responsive
  %table.table.table-striped.table-bordered.table-hover
    %thead
      %tr
        %th Email
    %tbody
      - @people.each do |person|
        %tr
          %td= person.email


= react_component 'Login'
= react_component 'Chats', chats: @chats
